<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>zotero_undo@jonathanwhiteley.com</em:id>
		<em:version>0.0</em:version>
		<em:type>2</em:type> <!-- type=extension -->
		
		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>2.1</em:minVersion>
				<em:maxVersion>3.0.*</em:maxVersion>
			</Description>
		</em:requires>
    
		<!-- Requires Firefox or Zotero Standalone -->
		<em:targetApplication>
			<Description>
				<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
				<em:minVersion>3.6</em:minVersion>
				<em:maxVersion>7.*</em:maxVersion>
			</Description>
		</em:targetApplication>
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>3.0b1</em:minVersion>
				<em:maxVersion>3.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>

        <!-- Front End MetaData -->
		<em:name>Undo Plugin for Zotero</em:name>
		<em:description>A plugin for Zotero to add basic Undo commands</em:description>
		<em:creator>Jonathan Whiteley<br/>McGill University</em:creator>
        <em:homepageURL>https://github.com/jawhiteley/</em:homepageURL>
        <em:updateURL>https://github.com/jawhiteley/</em:updateURL>
	</Description>
</RDF>
